---
import MapPage from "~/layouts/MapPage.astro";
import languages from '../../i18n/languages';
import * as br from "~/config/"


export const getStaticPaths = () => Object.keys(languages).map((lang) => ({ params: { lang } }));


---
<!-- start right out on a map page -->
<MapPage title={br.config.title} back={false}>
	
</MapPage>

<style>
	.container {
		display: grid;
		grid-template-columns: repeat(auto-fit, minmax(106px, 1fr));
	}
</style>
